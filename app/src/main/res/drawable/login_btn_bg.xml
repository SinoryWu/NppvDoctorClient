<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="295dp" android:height="52dp" android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#23c385" />
            <corners android:topLeftRadius="6dp" android:topRightRadius="6dp" android:bottomLeftRadius="6dp" android:bottomRightRadius="6dp" />

        </shape>
    </item>
    <item android:width="295dp" android:height="52dp" android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="#1FBE99" />
            <corners android:topLeftRadius="6dp" android:topRightRadius="6dp" android:bottomLeftRadius="6dp" android:bottomRightRadius="6dp" />

        </shape>
    </item>
</selector>